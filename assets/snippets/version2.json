        "description": "Función Val"
    },
    "subcadena-function": {
        "prefix": "subcadena",
        "body": "Subcadena(${1:variable} ,${Indice} ,${FinIndice})",
        "description": "Función Subcadena"
    },
    "buscar-function": {
        "prefix": "buscar",
        "body": "Buscar(${1:cadenaFuente} ,${cadena})",
        "description": "Función Buscar"
    },
    "reemplazar-function": {
        "prefix": "reemplazar",
